<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" version="1.0" elementFormDefault="qualified">
  <xs:element name="configuraciones">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="configuracion" minOccurs="1" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="nave" type="xs:string" minOccurs="1" maxOccurs="1"/>
            <xs:element name="modoDificil" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
          </xs:sequence>
          <xs:attribute name="nombre" type="xs:string" use="required"/>
        </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
